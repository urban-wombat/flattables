func ExampleNewFlatBuffersFromTableSet() {

//	Example (1) reading from a file and marshalling to FlatBuffers.

/*
	This is the setup.
	We will use the contents of "{{.GotablesFileName}}"
	A *gotables.TableSet is a set of one or more gotables tables,
	not necessarily stored in a file. See the file we used in the link below:

	https://github.com/urban-wombat/flattables_sample/blob/master/tables.got
*/
	var tableSet1 *gotables.TableSet
	var err error
	tableSet1, err = gotables.NewTableSetFromFile("{{.GotablesFileName}}")
	if err != nil { fmt.Println(err) }

/*
	Code in this "{{.NameSpace}}" package is generated by flatc using FlatBuffers
	schema "{{.SchemaFileName}}" which in turn is generated
	by flattablesc from gotables *gotables.TableSet "{{.GotablesFileName}}"

	Converting a set of tables to FlatBuffers is a single function call.
*/
	var flatTables *FlatTables
	var flatBuffers []byte
	flatTables, flatBuffers, err = NewFlatBuffersFromTableSet(tableSet1)
	if err != nil { fmt.Println(err) }
/*
	flatTables *FlatTables is the binary of the FlatTables root_type in the schema.
	flatBuffers []byte     is the FlatBuffers []byte data for storing/transmitting.

	That's all there is to it.
	You would probably ignore the flatTables variable unless you want to call
	FlatBuffers (flatc generated) code explicitly.
*/

	// Send flatBuffers off to a file or some other process ...

	// Here are some mildly interesting size statistics for the hell of it:

	tableSet1String := fmt.Sprintf("%s", tableSet1.String())
	flatTablesString := fmt.Sprintf("%#v", flatTables)
	flatBuffersString := fmt.Sprintf("%#v", flatBuffers)

	fmt.Printf("len(tableSet1)   = %4d\n", len(tableSet1String))
	fmt.Printf("len(flatTables)  = %4d\n", len(flatTablesString))
	fmt.Printf("len(flatBuffers) = %4d\n", len(flatBuffersString))

	fmt.Println()


//	Example (2) creating data on the fly and marshalling to FlatBuffers.

/*
	Create an empty *TableSet from the one we already have.
	We could set it up "programmatically" but this is easier.
*/
	tableSet2, err := tableSet1.Copy(false)
	if err != nil { fmt.Println(err) }

	// Populate MyXyzTable table

	xyzTable, err := tableSet2.Table("MyXyzTable")
	if err != nil { fmt.Println(err) }

	for rowIndex, num := 0, 42; num <= 44; rowIndex, num = rowIndex+1, num+1 {
		err = xyzTable.AppendRow()
		if err != nil { fmt.Println(err) }

		err = xyzTable.SetInt16("x", rowIndex, int16(num))
		if err != nil { fmt.Println(err) }

		err = xyzTable.SetFloat32("y", rowIndex, float32(num))
		if err != nil { fmt.Println(err) }

		err = xyzTable.SetFloat64("z", rowIndex, float64(num))
		if err != nil { fmt.Println(err) }
	}

	// Populate StringsAndThings table

	sntTable, err := tableSet2.Table("StringsAndThings")
	if err != nil { fmt.Println(err) }

	var rowIndex int
	rowIndex = 0
	err = sntTable.AppendRow()
	if err != nil { fmt.Println(err) }
	err = sntTable.SetString("flintstones", rowIndex, "Dino")
	if err != nil { fmt.Println(err) }
	err = sntTable.SetInt8("nums", rowIndex, 42)
	if err != nil { fmt.Println(err) }
	err = sntTable.SetString("spain", rowIndex, "Salvador")
	if err != nil { fmt.Println(err) }
	err = sntTable.SetBool("female", rowIndex, false)
	if err != nil { fmt.Println(err) }

	rowIndex = 1
	err = sntTable.AppendRow()
	if err != nil { fmt.Println(err) }
	// Hoppy is a pet hopparoo (a combination of a kangaroo and dinosaur)
	err = sntTable.SetString("flintstones", rowIndex, "Hoppy")
	if err != nil { fmt.Println(err) }
	err = sntTable.SetInt8("nums", rowIndex, 43)
	if err != nil { fmt.Println(err) }
	err = sntTable.SetString("spain", rowIndex, "Dali")
	if err != nil { fmt.Println(err) }
	err = sntTable.SetBool("female", rowIndex, false)
	if err != nil { fmt.Println(err) }
	// We "forgot" to set column "myui32", so it defaults to 0

	// Populate Wombats table

	wombatsTable, err := tableSet2.Table("Wombats")
	if err != nil { fmt.Println(err) }
	err = wombatsTable.AppendRow()
	if err != nil { fmt.Println(err) }
	rowIndex = 0
	err = wombatsTable.SetString("housingPolicy", rowIndex, "wide burrow")
	if err != nil { fmt.Println(err) }
	err = wombatsTable.SetInt8("topSpeedKmH", rowIndex, 42)
	if err != nil { fmt.Println(err) }
	err = wombatsTable.SetString("species", rowIndex, "Vombatus")
	if err != nil { fmt.Println(err) }
	err = wombatsTable.SetString("class", rowIndex, "Mammalia")
	if err != nil { fmt.Println(err) }
	err = wombatsTable.SetBool("wild", rowIndex, true)
	if err != nil { fmt.Println(err) }

	flatTables, flatBuffers, err = NewFlatBuffersFromTableSet(tableSet2)
	if err != nil { fmt.Println(err) }

	// Send flatBuffers off to a file or some other process ...

	// Here are some more mildly interesting size statistics for the hell of it:

	tableSet2String := fmt.Sprintf("%s", tableSet2.String())
	flatTablesString = fmt.Sprintf("%#v", flatTables)
	flatBuffersString = fmt.Sprintf("%#v", flatBuffers)

	fmt.Printf("len(tableSet2)   = %4d\n", len(tableSet2String))
	fmt.Printf("len(flatTables)  = %4d\n", len(flatTablesString))
	fmt.Printf("len(flatBuffers) = %4d\n", len(flatBuffersString))

	// Output:
	// len(tableSet1)   =  691
	// len(flatTables)  = 3224
	// len(flatBuffers) = 3152
	//
	// len(tableSet2)   =  481
	// len(flatTables)  = 2276
	// len(flatBuffers) = 2204
}

