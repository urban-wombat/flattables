{{range .Tables}}
// data table {{.TableIndex}}
table {{.Table.Name}} {
	{{range .Cols}}
		{{- .ColName}}: [{{.FbsType}}];	// Go type {{.ColType}}
	{{end}}
}
{{end}}

// root table contains all other tables.
table {{.TableSetName}} {
	{{range .Tables}}
		{{- .Table.Name}}:[ubyte] (nested_flatbuffer: "{{- .Table.Name}}");
	{{end}}
}

root_type {{.RootType}};
