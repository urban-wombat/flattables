namespace {{.NameSpace}};

{{range .Tables}}
// data table {{.TableIndex}}
table {{.Table.Name}} {
	{{range .Cols}}
		{{- .ColName}}: [{{.FbsType}}];	// Go type {{.ColType}}
	{{end}}
}
{{end}}

// root table. Contains all other (data) tables.
table FlatTables {
	{{range .Tables}}
		{{- .Table.Name}}:[ubyte] (nested_flatbuffer: "{{- .Table.Name}}");
	{{end}}
}

root_type FlatTables;
